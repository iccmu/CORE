[flake8]
max-line-length = 127
exclude =
    .git,
    __pycache__,
    migrations,
    venv,
    env,
    .venv,
    manage.py,
    settings.py,
    wsgi.py,
    asgi.py
ignore =
    E203,  # whitespace before ':'
    E501,  # line too long (handled by black)
    W503,  # line break before binary operator
max-complexity = 10








